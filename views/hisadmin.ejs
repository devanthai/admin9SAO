<div class="row">
    <div class="col-md-12">
        <div class="row">
            <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                <h3 class="font-weight-bold">Trang lịch sử admin
                </h3>

            </div>

        </div>
    </div>
</div>

<div class="row mt-2">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">

                <p class="text-danger card-title mb-0">Tài số lượng: <%-tais.length%> </p>
                <p class="text-danger card-title mb-0">Tùng số lượng: <%-tungs.length%> </p>


                
                <p class="card-title mb-0">Lịch sử admin</p>
                <div class="table-responsive">
                    <table class=" table table-striped table-borderless">
                        <thead>
                            <tr>
                                <th>Nội dung</th>
                                <th>Thời gian</th>
                              
                            </tr>
                        </thead>
                        <tbody>




                            <% if (lichsu.length<=0) { %>
                                <tr class="text-center">
                                    <td colspan="2">Không có đơn</td>
                                </tr>
                                <% } %>
                                    <% for (var i=0; i < lichsu.length; i++) { %>
                                        <tr>
                                         
                                            <td scope="col">
                                                <%= lichsu[i].noidung %>
                                            </td>

                                            <td scope="col">
                                                <%= new Date(Date.parse(lichsu[i].date)).toLocaleString()%>
                                            </td>

                                            

                                           
                                        </tr>


                                        <% } %>







                        </tbody>
                    </table>

                </div>
                
              
            </div>
        </div>
    </div>

</div>
